{
  "name": "tokyo-night-modern",
  "displayName": "Tokyo Night Modern",
  "description": "Beautiful modern dark theme for VS Code with carefully crafted colors and contemporary design",
  "version": "2.2.117",
  "author": "lod",
  "publisher": "lod-inc",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/darqus/tokyo-night-modern-vscode-theme"
  },
  "bugs": {
    "url": "https://github.com/darqus/tokyo-night-modern-vscode-theme/issues"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "tokyo",
    "night",
    "dark"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Tokyo Night Modern",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-modern-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build:theme": "ts-node scripts/build-theme.ts",
    "build:theme:watch": "ts-node-dev --respawn --watch src/theme,scripts/build-theme.ts scripts/build-theme.ts",
    "test:theme": "jest tests/theme-identical.test.ts",
    "test:theme:update": "jest tests/theme-identical.test.ts -u",
    "validate:theme": "ts-node scripts/validate-theme.ts",
    "build:vsix": "ts-node scripts/build-vsix.ts",
    "package": "npm run build:vsix",
    "release": "ts-node scripts/release.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "cleanup:palette": "ts-node scripts/cleanup-palette.ts"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vscode/vsce": "^3.6.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  }
}
