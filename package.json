{
  "name": "tokyo-night-lod",
  "displayName": "Tokyo Night Theme Collection",
  "description": "A comprehensive collection of Tokyo Night theme variants with adaptive palette system, featuring seasonal themes, accessibility options, and gradient variants.",
  "version": "1.11.0",
  "author": "lod",
  "publisher": "lod-inc",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "icon": "icon.png",
  "files": [
    "themes/",
    "icon.png",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/darqus/tokyo-night-vscode-theme-lod"
  },
  "bugs": {
    "url": "https://github.com/darqus/tokyo-night-vscode-theme-lod/issues"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "tokyo",
    "night",
    "dark",
    "storm",
    "moon",
    "seasonal",
    "spring",
    "summer",
    "autumn",
    "winter",
    "accessibility",
    "high-contrast",
    "low-contrast",
    "protanopia",
    "deuteranopia",
    "gradient",
    "pastel",
    "retro",
    "adaptive",
    "palette",
    "collection"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Tokyo Night Autumn",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-autumn-color-theme.json"
      },
      {
        "label": "Tokyo Night Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-dark-color-theme.json"
      },
      {
        "label": "Tokyo Night Deuteranopia Friendly",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-deuteranopia-friendly-color-theme.json"
      },
      {
        "label": "Tokyo Night Gradient 0",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-gradient-0-color-theme.json"
      },
      {
        "label": "Tokyo Night Gradient 1",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-gradient-1-color-theme.json"
      },
      {
        "label": "Tokyo Night Gradient 2",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-gradient-2-color-theme.json"
      },
      {
        "label": "Tokyo Night High Contrast",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-high-contrast-color-theme.json"
      },
      {
        "label": "Tokyo Night Low Contrast",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-low-contrast-color-theme.json"
      },
      {
        "label": "Tokyo Night Moon",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-moon-color-theme.json"
      },
      {
        "label": "Tokyo Night Pastel",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-pastel-color-theme.json"
      },
      {
        "label": "Tokyo Night Protanopia Friendly",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-protanopia-friendly-color-theme.json"
      },
      {
        "label": "Tokyo Night Retro",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-retro-color-theme.json"
      },
      {
        "label": "Tokyo Night Spring",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-spring-color-theme.json"
      },
      {
        "label": "Tokyo Night Storm",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-storm-color-theme.json"
      },
      {
        "label": "Tokyo Night Summer",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-summer-color-theme.json"
      },
      {
        "label": "Tokyo Night Winter",
        "uiTheme": "vs-dark",
        "path": "./themes/tokyo-night-winter-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "ts-node src/build.ts",
    "validate": "ts-node scripts/validate-theme.ts",
    "validate:all": "ts-node scripts/validate-theme.ts --all",
    "validate:summary": "ts-node scripts/validate-theme.ts --all --summary",
    "validate:fix": "ts-node scripts/validate-theme.ts --all --fix",
    "test": "npm run validate:all && npm run test:smoke && npm run test:unit && npm run test:visual",
    "test:unit": "jest tests/unit",
    "test:watch": "jest tests/unit --watch",
    "test:coverage": "jest tests/unit --coverage",
    "test:visual": "ts-node scripts/visual-test.ts",
    "test:visual:update": "ts-node scripts/visual-test.ts --update",
    "lint": "npm run lint:hex",
    "lint:hex": "ts-node scripts/lint-hex.ts",
    "lint:overlap": "ts-node scripts/check-overlaps.ts",
    "test:smoke": "ts-node scripts/smoke-compare.ts",
    "cli": "ts-node scripts/cli.ts",
    "debug": "ts-node scripts/theme-debug.ts",
    "setup": "ts-node scripts/setup-dev.ts",
    "analyze": "ts-node scripts/bundle-analyzer.ts",
    "package": "npm run generate:all && npm run update:package && vsce package",
    "publish": "vsce publish",
    "release": "ts-node scripts/smart-version.ts",
    "release:dry": "ts-node scripts/smart-version.ts --dry-run",
    "release:patch": "ts-node scripts/smart-version.ts --patch",
    "release:minor": "ts-node scripts/smart-version.ts --minor",
    "release:major": "ts-node scripts/smart-version.ts --major",
    "release:force": "ts-node scripts/smart-version.ts --force",
    "version": "ts-node scripts/smart-version.ts",
    "version:dry": "ts-node scripts/smart-version.ts --dry-run",
    "generate:themes": "ts-node src/generators/theme-generator.ts",
    "generate:all": "ts-node --transpile-only src/generators/theme-generator.ts --all",
    "generate:variants": "ts-node src/generators/theme-generator.ts --variants",
    "generate:seasonal": "ts-node src/generators/theme-generator.ts --seasonal",
    "generate:accessibility": "ts-node src/generators/theme-generator.ts --accessibility",
    "generate:demo": "ts-node src/generators/theme-generator.ts --demo",
    "theme-cli": "ts-node src/cli/theme-cli.ts",
    "export:palettes": "ts-node src/generators/theme-generator.ts --palettes",
    "update:package": "ts-node scripts/update-package-themes.ts",
    "build:complete": "npm run generate:all && npm run update:package && npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.10",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/puppeteer": "^5.4.7",
    "jest": "^29.7.0",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "puppeteer": "^24.17.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}