  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation",
        "comment.line.double-slash",
        "comment.line.number-sign"
      ],
      "settings": {
        "foreground": "#4a5a70",
        "fontStyle": ""
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.unquoted",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#9ece6a"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "variable.other.constant.number"
      ],
      "settings": {
        "foreground": "#e8b892"
      }
    },
    {
      "name": "Booleans",
      "scope": [
        "constant.language.boolean",
        "keyword.constant"
      ],
      "settings": {
        "foreground": "#d5a8d6"
      }
    },
    {
      "name": "Null and Undefined",
      "scope": [
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#d5a8b6"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.delete",
        "keyword.operator.sizeof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike"
