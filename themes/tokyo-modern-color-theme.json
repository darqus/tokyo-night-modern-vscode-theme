  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#939bc4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Number, Boolean, Undefined, Null",
      "scope": [
        "constant.numeric",
        "constant.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#cc936e"
      }
    },
    {
      "name": "String, Symbols",
      "scope": ["string", "constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#79d68c"
      }
