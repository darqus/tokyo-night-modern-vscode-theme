    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#acc1e5"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#f33f8a"
      }
    },
    {
      "name": "Storage Type",
      "scope": "storage.type",
      "settings": {
