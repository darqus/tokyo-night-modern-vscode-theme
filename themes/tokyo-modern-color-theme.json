  "tokenColors": [
    {
      "name": "Italics - Comments, Storage, Keyword Flow",
      "scope": [
        "comment",
        "keyword.control.flow",
        "keyword.control.return",
        "storage.modifier"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7284a6"
      }
    },
    {
      "name": "Number, Boolean, Undefined, Null",
      "scope": [
        "constant.numeric",
        "constant.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#e0945a"
      }
    },
    {
      "name": "String, Symbols",
      "scope": ["string", "constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#79d68c"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#bacaf8"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#e98864"
      }
    },
    {
      "name": "Storage Type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#f88d7a"
      }
    },
    {
      "name": "Storage - modifier, var, const, let",
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#9976db"
      }
    },
    {
      "name": "Interpolation, PHP tags",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#a0a0ff"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ff6b7a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#b0b9fb"
      }
    },
    {
      "name": "Template & Misc",
      "scope": ["keyword.other", "punctuation.definition.keyword"],
      "settings": {
        "foreground": "#c2c07d"
      }
    },
    {
      "name": "Import, Export, From, Default",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default"
      ],
      "settings": {
        "foreground": "#4de0d8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control"],
      "settings": {
        "foreground": "#c2a9ff"
      }
    },
    {
      "name": "Keyword Operator Logical",
      "scope": [
        "keyword.operator.logical",
        "keyword.operator.bitwise",
        "keyword.operator.comparison",
        "keyword.operator.relational"
      ],
      "settings": {
        "foreground": "#ff7d8a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#9976db"
      }
    },
    {
      "name": "Tag Punctuation",
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#a8bee4"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#99cc40"
      }
    },
    {
      "name": "Function Arguments",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#7a7aff"
      }
    },
    {
      "name": "Object Key",
